<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.abstractmgs.label.repository.AnswerRepository">
    <resultMap id="answerMapper"
               extends="cn.abstractmgs.BaseEntityMapper.baseEntityMapper"
               type="cn.abstractmgs.label.model.entity.Answer">
        <id property="id" column="answer_id"/>
        <result property="text" column="text"/>
        <result property="startIndex" column="start_index"/>
        <association property="question.id" column="question_id" fetchType="lazy"
                     select="cn.abstractmgs.label.repository.QuestionRepository.selectById"/>
    </resultMap>
</mapper>